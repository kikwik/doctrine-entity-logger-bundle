{
    "name": "kikwik/doctrine-entity-logger-bundle",
    "description": "Listener for doctrine entity changes",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Davide Scarpato",
            "email": "davide@kikwik.it"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "doctrine/orm": "3.*",
        "doctrine/doctrine-bundle": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Kikwik\\DoctrineEntityLoggerBundle\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "symfony/yaml": "^6.4|^7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Kikwik\\DoctrineEntityLoggerBundle\\Tests\\": "tests/"
        }
    }
}
